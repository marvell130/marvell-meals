{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true,
    "jasmine": true
  },
  "extends": ["airbnb-base", "plugin:codeceptjs/recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "operator-linebreak": "off",
    "comma-dangle": ["error", "never"]
  }
}
